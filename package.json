{
  "name": "reviews",
  "version": "1.0.0",
  "description": "",
  "author": "Pamela Ling",
  "license": "",
  "engines": {
    "node": ">=6.13.0"
  },
  "scripts": {
    "react-dev": "webpack -d --watch",
    "server-dev": "nodemon server/index.js",
    "start": "node server/index.js",
    "create": "mysql -u root < database/schema.sql",
    "seed": "node database/seed.js",
    "test-watch": "jest --watchAll --config jest.config.js",
    "test-coverage": "jest --verbose --coverage",
    "csv-users": "node --max-old-space-size=9000 database/csv/usersCsvGenerator.js",
    "csv-restaurants": "node --max-old-space-size=9000 database/csv/restaurantsCsvGenerator.js",
    "csv-reviews": "node --max-old-space-size=9000 database/csv/reviewsCsvGenerator.js",
    "csv-all": "npm run csv-users && npm run csv-restaurants && npm run csv-reviews",
    "drop-mongo": "mongo < database/mongo/dropDb.txt",
    "start-mongo": "mongo < database/mongo/startMongo.txt",
    "schema-mongo": "npm run drop-mongo && node database/mongo/schema.js",
    "seed-mongo-users": "mongoimport --db reviewsDB --collection users --type csv --headerline --file /Users/syoh/Desktop/SDC/Reviews/database/csv/users.csv",
    "seed-mongo-restaurants": "mongoimport --db reviewsDB --collection restaurants --type csv --headerline --file /Users/syoh/Desktop/SDC/Reviews/database/csv/restaurants.csv",
    "seed-mongo-reviews": "mongoimport --db reviewsDB --collection reviews --type csv --headerline --file /Users/syoh/Desktop/SDC/Reviews/database/csv/reviews.csv",
    "seed-mongo-all": "npm run seed-mongo-users && npm run seed-mongo-restaurants && npm run seed-mongo-reviews",
    "schema-postgres": "psql reviewsDB < database/postgres/schema.sql",
    "seed-postgres-users": "psql reviewsDB < database/postgres/seedUsers.sql",
    "seed-postgres-restaurants": "psql reviewsDB < database/postgres/seedRestaurants.sql",
    "seed-postgres-reviews": "psql reviewsDB < database/postgres/seedReviews.sql",
    "seed-postgres-all": "npm run seed-postgres-users && npm run seed-postgres-restaurants && npm run seed-postgres-reviews",
    "schema-all": "npm run schema-mongo && npm run schema-postgres",
    "seed-all-users": "npm run seed-postgres-users && npm run seed-mongo-users",
    "seed-all-restaurants": "npm run seed-postgres-restaurants && npm run seed-mongo-restaurants",
    "seed-all-reviews": "npm run seed-postgres-reviews && npm run seed-mongo-reviews",
    "seed-all-all": "npm run seed-postgres-all && npm run seed-mongo-all",
    "get-her-done": "npm run csv-all && npm run seed-all-all",
    "index-mongo": "mongo < database/mongo/index-mongo.txt"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "babel-jest": "^24.9.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "csv-write-stream": "^2.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "file-system": "^2.2.2",
    "fs": "0.0.1-security",
    "jest": "^24.9.0",
    "jest-config": "^24.9.0",
    "jest-styled-components": "^6.3.3",
    "moment": "^2.24.0",
    "mongodb": "^3.3.2",
    "mongoose": "^5.7.0",
    "mongoose-auto-increment": "^5.0.1",
    "mysql": "^2.17.1",
    "newrelic": "^5.11.0",
    "nodemon": "^1.19.1",
    "parser": "^0.1.4",
    "pg": "^7.12.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-paginate": "^6.3.0",
    "react-star-ratings": "^2.3.0",
    "styled-components": "^4.3.2",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "eslint-config-hackreactor": "git://github.com/reactorcore/eslint-config-hackreactor",
    "url-loader": "^2.1.0"
  }
}
